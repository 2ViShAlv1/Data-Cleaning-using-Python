    "def clean_data(data) :\n",
    "    #remove users with empty names\n",
    "    data['users'] = [user for user in user['name'] if user['name'].strip()]\n",
    "\n",
    "    #remove duplicate friends\n",
    "    for user in data['user'] :\n",
    "        user['friends'] = list(set())\n",
    "    return data"
   ]
  },
